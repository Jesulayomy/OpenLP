<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2022-04-17T08:46:07">
  <properties>
    <titles>
      <title>I Lift My Hands</title>
    </titles>
    <authors>
      <author type="words">Louie Giglio</author>
      <author type="music">Louie Giglio</author>
      <author type="words">Chris Tomlin</author>
      <author type="music">Chris Tomlin</author>
      <author type="words">Matt Maher</author>
      <author type="music">Matt Maher</author>
    </authors>
    <songbooks>
      <songbook name="Crown Songbook"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>I lift my hands to the coming King,<br/>To the great I AM, to You I sing,<br/>For You're the One who reigns within my heart.</lines>
    </verse>
    <verse name="v2">
      <lines>And I will serve no foreign god,<br/>Or any other treasure;<br/>You are my heart's desire,<br/>Spirit without measure.<br/>Unto Your name<br/>I will bring my sacrifice.</lines>
    </verse>
  </lyrics>
</song>
