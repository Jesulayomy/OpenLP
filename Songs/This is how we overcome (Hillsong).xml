  <properties>
    <titles>
      <title>This is how we overcome</title>
    </titles>
    <authors>
      <author type="words">Hillsong</author>
    </authors>
    <themes>
      <theme>dance</theme>
      <theme>keyboard</theme>
      <theme>joy</theme>
      <theme>response</theme>
      <theme>piano</theme>
      <theme>guitar</theme>
      <theme>praise</theme>
      <theme>meditation</theme>
      <theme>longing</theme>
      <theme>devotion</theme>
      <theme>grace</theme>
      <theme>difficulties</theme>
      <theme>blessing</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Your light broke through my night,<br/>Restored exceeding joy.<br/>Your grace fell like the rain,<br/>And made this desert live.</lines>
    </verse>
    <verse name="c1">
      <lines>You have turned  my mourning into dancing.<br/>You have turned  my sorrow into joy.</lines>
    </verse>
    <verse name="v2">
      <lines>Your hand lifted me up,<br/>I stand on higher ground.<br/>Your praise rose in my heart,<br/>And made this valley sing.</lines>
    </verse>
  </lyrics>
</song>
