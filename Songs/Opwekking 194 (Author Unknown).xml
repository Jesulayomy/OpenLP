<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2022-04-17T08:46:16">
  <properties>
    <titles>
      <title>Opwekking 194</title>
      <title>U maakt ons één</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Rising Tabernakel"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>U maakt ons één,<br/>U bracht ons tesamen,<br/>wij eren en aanbidden U.<br/>(x2)</lines>
    </verse>
    <verse name="c1">
      <lines>Wordt uw wil gedaan,<br/>dan bindt het ons saam,<br/>iedereen zal deel zijn van uw gezin.<br/>(x2)</lines>
    </verse>
  </lyrics>
</song>
