  <properties>
    <titles>
      <title>Holy</title>
    </titles>
    <authors>
      <author type="words">Matt Gilman</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Only one word comes to mind<br/>There's only one word to describe</lines>
