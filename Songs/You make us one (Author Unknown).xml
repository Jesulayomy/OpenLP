    <titles>
      <title>You make us one</title>
      <title>Opwekking 194</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Rising Tabernakel"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>You make us one<br/>You bring us together<br/>We honor and we worship You<br/>(x2)</lines>
    </verse>
    <verse name="c1">
      <lines>When Your will is done<br/>It binds us as one<br/>We will all be part of Your family<br/>(x2)</lines>
    </verse>
  </lyrics>
