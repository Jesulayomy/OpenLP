<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2022-04-17T08:46:26">
  <properties>
    <titles>
      <title>He is our peace</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Rising Tabernakel"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>He is our peace<br/>who has broken<br/>down every wall.<br/>He is our Peace,<br/>He is our Peace. (2x)</lines>
    </verse>
    <verse name="v2">
      <lines>Cast all your<br/>cares on Him<br/>for He careth for you.<br/>He is our peace,<br/>He is our peace. (2x)</lines>
    </verse>
  </lyrics>
</song>
