  <properties>
    <titles>
      <title>Arise Shine</title>
      <title>Thy Light is come</title>
    </titles>
    <authors>
      <author type="words">Kent Henry</author>
      <author type="music">Kent Henry</author>
    </authors>
  </properties>
  <lyrics>
